		print(rules[pg],32,20,7)
	elseif(pg>=7) then
		print(rules[pg],32,20,8)
	end
end

function draw_they_eat_these()
 ruletick += 1
 for e in all(eaten) do
  if (ruletick %10 == 0) then
  e.cs += 16
  if e.cs > e.s + 48 then
   e.cs = e.s
  end
 end
  spr(e.cs,e.x,
  e.y+3*sin(e.s/8*time()))
 end
end

