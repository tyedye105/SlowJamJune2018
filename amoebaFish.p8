end

function draw_sprites()

	if(pg==1) then
 spr(1,44+sin(time()*2),60+3*sin(time()*.8),2,1)
 elseif (pg==2) then
 spr(18,48+sin(time()*2),60+3*sin(time()*.8),2,1)
