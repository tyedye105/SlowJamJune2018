function update_events()
 for c in all(events) do
    if costatus(c) != "dead" then
      coresume(c)
    else
      del(actions,c)
    end
  end
end


function level_up()
 lvl = flr(hooked/10)
 if (hooked > 0 and hooked %10 == 0) then
  max_critters += 3
  spawn_timer = 0
  add_critter()
  spawn_timer = 0
  add_critter()
  spawn_timer = 0
  add_critter()
  
  spawn_time -=5
 end
end

function check_end()
 if (is_clogged()) then
  _draw = game_over_draw
  _update = game_over_update
 elseif health >= 100 then
  _update = game_over_update
  _draw = game_win_draw
 end
end

